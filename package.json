{
  "name": "adopt-me",
  "version": "1.0.0",
  "description": "An app to teach you to write React",
  "main": "src/App.js",
  "homepage": "https://design-computing.github.io/courseOrg/mugs",
  "scripts": {
    "test": "jest --silent",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "dev": "parcel src/index.html --out-dir ../docs/mug",
    "build": "parcel build src/index.html --out-dir ../docs/mug",
    "treex": "depcruise --exclude \"^node_modules\" --output-type dot src | dot -T svg > dependencyGraph.svg",
    "tree": "depcruise --exclude \"^node_modules\" --output-type dot src > dependencyGraph.dot"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!petfinder-client).+"
    ],
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btholt/complete-intro-to-react-v4.git"
  },
  "author": "Brian Holt <btholt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/btholt/complete-intro-to-react-v4/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "cross-fetch": "^3.0.2",
    "enzyme": "^3.9.0",
    "gh-pages": "^2.0.1",
    "js-yaml": "^3.13.1",
    "luxon": "^1.12.1",
    "material-design-lite": "^1.3.0",
    "petfinder-client": "^2.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.7",
    "react-test-renderer": "^16.4.2"
  }
}
